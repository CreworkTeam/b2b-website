---
import MainLayout from '../layouts/main-layout.astro';
import Hero from '../components/hero.astro';
import OurImpact from '../components/impact.astro';
import OurPortfolio from '../components/portfolio.astro';
import CaseStudies from '../components/case-studies.astro';
import Contact from '../components/contact.astro';
---

<MainLayout
  title="Crework Labs"
  description="We are a product studio deeply focused on crafting experiences that connect you with your audience."
  canonical="https://www.creworklabs.com"
>
  <Hero />
  <OurPortfolio />
  <OurImpact />
  <CaseStudies />
  <Contact />
  <script>
    import { setupButtonTracking } from '../utils/mixpanel';

    setupButtonTracking({
      pageName: 'LandingPage',
    });
  </script>
</MainLayout>
